{
  "name": "goRead",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Go Read!",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com/analytics.js; object-src 'self'",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "goRead!",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
  ],
  "options_ui": {
    "page": "pages/options.html",
    "chrome_style": true,
    "open_in_tab": true
  }
}